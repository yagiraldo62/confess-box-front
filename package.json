    "axios": "^1.3.5",
    "dotenv": "^16.0.3",
    "next": "latest",
    "next-redux-wrapper": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-login": "^5.2.2",
